    <t t-name="DashboardChart">

<div class="row mt-5">

    <div class="upcoming_activities_div col-12 col-sm-12 col-md-4 calls">
                    <div class="chart-container card-shadow">
                        <h3 class="h5">Call</h3>
                        <hr/>
                        <div class="crm_scroll_table">
                            <div class="items-table">
                                <div class="item-container">
                                    <t t-foreach="widget.get_recent_call_activities" t-as="activity">
                                    <div class="item-header">
                                        <div class="count-container">
                                            <t t-esc="activity[1]"/>
                                        </div>
                                        <div class="item-title pl-3">
                                            <div class="item-content">
                                                <ul>
                                                    <li>
                                                        Activity:
                                                        <span style="font-size: 16px;color: #4c4c4c;">
                                                            <t t-esc="activity[4]"/>
                                                        </span>
                                                    </li>
                                                    <li>
                                                        Name:
                                                        <span style="font-size: 15px;color: #4c4c4c;">
                                                            <t t-esc="activity[3]"/>
                                                        </span>
                                                    </li>
                                                    <li t-if="activity[2]">Summary:
                                                        <span style="font-size: 13px;color: #4c4c4c;">
                                                            <t t-esc="activity[2]"/>
                                                        </span>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    </t>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
    <div class="upcoming_activities_div col-12 col-sm-12 col-md-4 meeting">
                    <div class="chart-container card-shadow">
                        <h3 class="h5">Meeting</h3>
