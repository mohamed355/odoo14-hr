<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="hr_applicant_view_form_inherit" model="ir.ui.view">
        <field name="name">hr_recruitment.hr_applicant_view_form.inherit</field>
        <field name="model">hr.applicant</field>
        <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[1]" position="after">
<page string="Interview">
    <group string="Date">
        <group>
            <field name="interview_date"/>
        </group>
        <group/>
    </group>
    <group>
        <group>
            <field name="re_skill"/>
            <field name="ro_skill"/>
            <field name="v_skill"/>
            <field name="w_skill"/>
            <field name="a_skill"/>
            <field name="t_skill"/>
            <field name="i_skill"/>
            <field name="c_skill"/>
            <field name="s_skill"/>
            <field name="l_skill"/>
            <field name="in_skill"/>
            <field name="att_skill"/>
            <field name="atc_skill"/>
            <field name="pro_skill"/>
            <field name="ca_skill"/>
        </group>
        <group>
            <field name="skill_re_s"/>
            <field name="skill_ro_s"/>
            <field name="skill_v_s"/>
            <field name="skill_w_s"/>
            <field name="skill_a_s"/>
            <field name="skill_t_s"/>
            <field name="skill_i_s"/>
            <field name="skill_c_s"/>
            <field name="skill_s_s"/>
            <field name="skill_l_s"/>
            <field name="skill_in_s"/>
            <field name="skill_att_s"/>
            <field name="skill_atc_s"/>
            <field name="skill_pro_s"/>
            <field name="skill_ca_s"/>
        </group>

    </group>
    <group>
        <group>
            <field name="inter_by"/>
            <field name="re_action"/>
        </group>
        <group/>
    </group>
</page>
            </xpath>
        </field>
    </record>
</odoo>
